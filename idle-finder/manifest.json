{

  "manifest_version": 2,
  "name": "Idle Finder",
  "version": "1.0",

  "description": "finds stuff",
  "icons": {
    "48": "icons/alarms-48.png"
  },

  "permissions": [
    "find"
	"idle"
  ],

  "browser_action": {
    "default_icon": "icons/alarms-32.png",
